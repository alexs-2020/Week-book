<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200" prefWidth="200.0">
      <children>
         <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="20.0">
            <children>
               <Button fx:id="newFile" mnemonicParsing="false" text="New File">
                  <opaqueInsets>
                     <Insets />
                  </opaqueInsets>
               </Button>
               <Button fx:id="openFileStart" mnemonicParsing="false" text="Open Existing File" />
            </children>
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
         </VBox>
      </children></AnchorPane>
</Scene>
